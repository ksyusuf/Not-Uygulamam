{
  "version": 2,
  "builds": [
    {
      "src": "server/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    }
  ],
  "env": {
    "CORS_ORIGIN": "https://not-uygulamasi-client.vercel.app",
    "MONGODB_URI": "@MONGODB_URI",
    "NODE_ENV": "production"
  },
  "git": {
    "deploymentEnabled": false
  }
}
